{
  "name": "express-react-ts-v0.0.1",
  "version": "0.0.1",
  "main": "_srvmain.js",
  "author": "Chysev",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch src --watch src/pages --ext ts,tsx,css --exec 'yarn build && babel-watch build/_srvmain.js'",
    "build": "tsc --rootDir src --module es2020 --outDir build --jsx preserve && yarn publicpath && yarn css && yarn views",
    "rebuild": "rm -rf build && yarn build",
    "publicpath": "babel src/public --out-dir build/public --copy-files",
    "css": "babel src/css --out-dir build/css --copy-files",
    "views": "babel views --out-dir build/views --copy-files",
    "test": "node build/_app.js",
    "up": "pm2 start build/_app.js",
    "down": "pm2 stop build/_app.js"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.20.7",
    "@babel/plugin-transform-react-jsx": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/register": "^7.21.0",
    "@types/reload": "^3.2.0",
    "babel-node": "^0.0.1-security",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "babel-plugin-react-require": "^3.1.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "jsx-transform": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reload": "^3.2.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@types/babel__register": "^7.17.0",
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.1",
    "babel-watch": "^7.7.2",
    "nodemon": "^2.0.22",
    "typescript": "^5.0.4"
  }
}